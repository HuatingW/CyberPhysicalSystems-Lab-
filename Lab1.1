void setup() {
  // put your setup code here, to run once:
  pinMode(6,OUTPUT);
  Serial.begin(9600);
  analogReadResolution(12);// read 12-bit
  while(!Serial);// wait for serial monitor
}

void loop() {
  // put your main code here, to run repeatedly:
   digitalWrite(6, HIGH);
   delay(500);
   digitalWrite(6,LOW);
   delay(500);

   int raw= analogRead(A1); // 12-bit: 0 to 4095
   float voltage= raw *(3.3/ 4095.0);

   Serial.print("Analog value: ");
   Serial.print(raw);
   Serial.print(" -> Voltage: ");
   Serial.print(voltage,3);
   Serial.println("V");

   delay(1000);
}
